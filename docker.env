# ArRahnu Auction Online - Basic Docker Environment

APP_NAME="ArRahnu Auction Online"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://arrahnuauction.muamalat.com.my
APP_TIMEZONE=Asia/Kuala_Lumpur

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=arrahnu_auction
DB_USERNAME=arrahnu_user
DB_PASSWORD=user123

# Cache and Session
CACHE_DRIVER=file
SESSION_DRIVER=file
QUEUE_CONNECTION=sync

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.muamalat.com.my
MAIL_PORT=587
MAIL_USERNAME=noreply@arrahnuauction.muamalat.com.my
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@arrahnuauction.muamalat.com.my
MAIL_FROM_NAME="${APP_NAME}"

# File Storage
FILESYSTEM_DISK=local

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=warning

# Security
SESSION_LIFETIME=120
